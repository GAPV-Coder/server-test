{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "airbnb-base"],
    "rules": {
        "prettier/prettier": "warn",
        "linebreak-style": ["error", "windows"]
    },
    "prettier/prettier": [
        "error",
        {
            "endOfLine": "auto"
        }
    ]
}
